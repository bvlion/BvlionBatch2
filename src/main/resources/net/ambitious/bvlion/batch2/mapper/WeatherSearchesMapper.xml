<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.ambitious.bvlion.batch2.mapper.WeatherSearchesMapper">

	<resultMap id="entityResultMap" type="net.ambitious.bvlion.batch2.entity.WeatherSearchesEntity">
		<result property="areaName" column="area_name" />
		<result property="pcUrl" column="pc_url" />
		<result property="mobileUrl" column="mobile_url" />
		<result property="userAgent" column="user_agent" />
	</resultMap>

	<select id="selectWeatherSearchList" resultMap="entityResultMap">
		SELECT area_name, pc_url, mobile_url, user_agent FROM weather_searches WHERE enable_flag = 1
	</select>
</mapper>
